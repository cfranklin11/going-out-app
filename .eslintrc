{
  "extends": ["google", "plugin:react/recommended"],
  "plugins": [
    "react"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "installedESLint": true,
  "env": {
    "browser": true
  },
  "globals": {
    "React": true,
    "alt": true
  },
  "rules": {
    "require-jsdoc": 0,
    "camelcase": 0,
    "space-before-function-paren": ["error", "always"],
    "object-curly-spacing": ["error", "always"]
  }
}
